            <asp:ContentPlaceHolder ID="MainContent" runat="server"></asp:ContentPlaceHolder>
            <!-- Separador horizontal -->
            <hr />
            <!-- Pie de página -->
            <footer>
                <!-- Aviso de copyright con el año actual -->
                <p>&copy; <%: DateTime.Now.Year %> - TPC | Grupo G</p>
            </footer>
