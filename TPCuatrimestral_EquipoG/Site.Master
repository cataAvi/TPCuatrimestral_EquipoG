                            <!-- Opciones del menú desplegable -->
                            <ul class="dropdown-menu">
                                <!-- Opción de cerrar sesión -->
                                <li><a class="dropdown-item" href="IniciarSesionaspx.aspx">Iniciar sesión</a></li>
